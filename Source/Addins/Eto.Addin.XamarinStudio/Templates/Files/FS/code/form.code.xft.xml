<?xml version="1.0"?>
<Template>
	<TemplateConfiguration>
		<_Name>Form</_Name>
		<Icon>md-text-file-icon</Icon>
		<_Category>Eto.Forms</_Category>
		<LanguageName>F#</LanguageName>
		<_Description>
			<![CDATA[Creates a new cross platform non-modal Form class using Eto.Forms in F#.]]>
		</_Description>
		<DefaultFilename>MyForm</DefaultFilename>
		<GroupId>eto-file-form</GroupId>
	</TemplateConfiguration>
	<TemplateFiles>
        <File name="${Name}.fs" AddStandardHeader="true" src="form.code.fs"/>
	</TemplateFiles>
</Template>
